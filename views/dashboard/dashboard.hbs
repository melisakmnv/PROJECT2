<section id="dashboard-carousel-container">

<h1>MY DASHBOARD</h1>

<a href="/dashboard/mywishlist" class="gotowish">Go to my Wishlist</div>

<div class="destinations-container-dashboard">

    <div class="grid-container-dashboard">

    {{#each user.wishlist}}

		<div class="photo-and-cityname-dashboard">
		
      <img class="city-thumbnail-wishlist" src="{{this.city_photo}}" alt="Photo of {{this.city_name}}"/>

		</div>

    {{/each}}
        
    </div>


</div>

</section>







<section id="dashboard-menu">

<a href="/map" class="dashboard-options-one">
    <img class="dashboard-img-1" src="/images/dashboard_map-icon.png" alt="Map icon">
    <h2>Access map</h2>
</a>

<a href="../dashboard/destinations_add" class="dashboard-options-two">
    <img class="dashboard-img-2" src="/images/dashboard_add-destination-icon.png" alt="Add destination icon">
    <h2>Add a place or destination</h2>
</a>

<a href="/dashboard/myprofile" class="dashboard-options-three">
    <img class="dashboard-img-3" src="/images/dashboard_myprofile-icon.png" alt="My profile icon">
    <h2>My profile</h2>
</a>

</section>